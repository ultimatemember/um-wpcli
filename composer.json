{
    "name": "ultimatemember/um-wpcli",
    "description": "Ultimate Member's Integration with WP CLI",
    "type": "wordpress-plugin",
    "homepage": "https://ultimatemember.com/",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
       {
          "name": "Ultimate Member Group Ltd.",
          "email": "team@ultimatemember.com"
       }
    ],
    "autoload": {
       "psr-4": {
          "UM_WPCLI\\": "src"
       }
    },
    "require-dev": {
      "wp-cli/wp-cli": "*",
      "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
      "object-calisthenics/phpcs-calisthenics-rules": "*",
      "phpcompatibility/phpcompatibility-wp": "*",
      "wp-coding-standards/wpcs": "2.3.0",
      "squizlabs/php_codesniffer": "3.*",
      "phpdocumentor/phpdocumentor": "3.1.*"
   },
   "config": {
      "allow-plugins": {
         "symfony/flex": true,
         "dealerdirect/phpcodesniffer-composer-installer": true
      }
   }
   
 }
